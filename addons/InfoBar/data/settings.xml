<?xml version="1.1" ?>
<settings>
    <global>
        <NoTarget>${name} (${main_job}${main_job_level}/${sub_job}${sub_job_level}) (${x},${y},${z})</NoTarget>
        <TargetMOB>${name}</TargetMOB>
        <TargetNPC>${name}</TargetNPC>
        <TargetPC>${name}</TargetPC>
        <display>
            <bg>
                <alpha>102</alpha>
                <blue>0</blue>
                <green>0</green>
                <red>0</red>
                <visible>true</visible>
            </bg>
            <flags>
                <bold>false</bold>
                <bottom>false</bottom>
                <draggable>true</draggable>
                <italic>false</italic>
                <right>false</right>
            </flags>
            <padding>0</padding>
            <pos>
                <x>0</x>
                <y>0</y>
            </pos>
            <text>
                <alpha>255</alpha>
                <blue>255</blue>
                <font>Consolas</font>
                <fonts>

                </fonts>
                <green>255</green>
                <red>255</red>
                <size>12</size>
                <stroke>
                    <alpha>255</alpha>
                    <blue>0</blue>
                    <green>0</green>
                    <red>0</red>
                    <width>0</width>
                </stroke>
            </text>
        </display>
    </global>
    <khrone>
        <display>
            <pos>
                <x>-1</x>
                <y>-3</y>
            </pos>
            <text>
                <red>128</red>
            </text>
        </display>
    </khrone>
    <treubond>
        <display>
            <pos>
                <x>-1</x>
            </pos>
        </display>
    </treubond>
    <treuse>
        <display>
            <pos>
                <x>-1</x>
                <y>-2</y>
            </pos>
            <text>
                <blue>128</blue>
                <red>128</red>
            </text>
        </display>
    </treuse>
</settings>
